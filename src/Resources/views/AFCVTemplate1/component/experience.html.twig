<div class="row">
    <div class="col">
        <h2>Work Experience</h2>
    </div>
</div>
<div class="row">
    <div class="col">
        <ul class="experiences">
        {% for experience in cvInfo.experiences %}
            <li>
                <div class="row">
                    <h3>{{ experience.title }}</h3>
                </div>
                <div class="row">
                    <p>{{ experience.description }}</p>
                </div>
            </li>
        {% endfor %}
        </ul>
    </div>
</div>
